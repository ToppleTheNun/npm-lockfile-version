{
  "extends": [
    "config:js-lib",
    "group:linters",
    "group:postcss",
    "group:test",
    "npm:unpublishSafe",
    ":semanticCommits"
  ],
  "labels": [
    "automerge"
  ],
  "postUpgradeTasks": {
    "commands": ["yarn build", "yarn package"],
    "fileFilters": ["package.json", "yarn.lock", "dist/**"],
    "executionMode": "update"
  },
  "prCreation": "not-pending"
}
